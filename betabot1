import discord
from discord.ext import commands
#The bot's personal Token
TOKEN = '*****************************'

client = commands.Bot(command_prefix='.')

#Once the bot is all ready it prints a ready-message
@client.event
async def on_ready():
    print('MOMMY is ready to face the dangers of the discord server')

#When a member joins any server the bot is in it prints a message
@client.event
async def on_member_join(member):
    print(f'{member} has joined a server that Gang Weed is in.')

#When a member is removed from any server the bot is in it prints a message
@client.event
async def on_member_remove(member):
    print(f'{member} has left a server that Gang Weed is in.')


#This one is a funny censoring message; if it detects certain keywords it sends a threatening message
@client.event
async def on_message(message):
    if client.user.id != message.author.id:
        message_content = message.content.lower()
        if 'i dont like' in message_content or "i don't like" in message_content or 'i hate' in message_content or 'fuck' in message_content or 'is stupid' in message_content or 'is gay' in message_content or 'is dumb' in message_content or 'is retarded' in message_content or 'is a bitch' in message_content or 'sucks' in message_content:
            if 'jahseh' in message_content or 'jah' in message_content:
                await message.channel.send(f'''Ok buddy, {message.author}. 
\nYou don't like Jah? Well hes kind of a god around these parts so you better be watching your god damn mouth, got it retard? 
\nNext time im going to have to punish you. 
\nJesus Christ.''')

