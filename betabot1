#This listens for people to ask how many GBP someone has, or listens to display a log of GBP via the keyword 'gbp log'
@client.listen()
async def on_message(message):
    global Nico
    global Eli
    global Ashton
    global Lily
    global Enrique
    global Kylie
    global Abbie
    global Luke
    global Brandon
    global Charlie
    global Dan
    global JJ
    global Michael
    global Zameer
    global Logan
    global HAXY

    if 'how many gbp does nico have' in message.content.lower() != -1:
        await message.channel.send(f'''Nico currently has {Nico} GBP''')

    if 'how many gbp does eli have' in message.content.lower() != -1:
        await message.channel.send(f'''Eli currently has {Eli} GBP''')

    if 'how many gbp does ashton have' in message.content.lower() != -1:
        await message.channel.send(f'''Ashton currently has {Ashton} GBP''')

    if 'how many gbp does lily have' in message.content.lower() != -1:
        await message.channel.send(f'''Lily currently has {Lily} GBP''')

    if 'how many gbp does enrique have' in message.content.lower() != -1:
        await message.channel.send(f'''Enrique currently has {Enrique} GBP''')

    if 'how many gbp does kylie have' in message.content.lower() != -1:
        await message.channel.send(f'''Kylie currently has {Kylie} GBP''')

    if 'how many gbp does abbie have' in message.content.lower() != -1:
        await message.channel.send(f'''Abbie currently has {Abbie} GBP''')

    if 'how many gbp does luke have' in message.content.lower() != -1:
        await message.channel.send(f'''Luke currently has {Luke} GBP''')

    if 'how many gbp does brandon have' in message.content.lower() != -1:
        await message.channel.send(f'''Brandon currently has {Brandon} GBP''')

    if 'how many gbp does charlie have' in message.content.lower() != -1:
        await message.channel.send(f'''Charlie currently has {Charlie} GBP''')

    if 'how many gbp does nico have' in message.content.lower() != -1:
        await message.channel.send(f'''Dan currently has {Dan} GBP''')

    if 'how many gbp does jj have' in message.content.lower() != -1:
        await message.channel.send(f'''JJ currently has {JJ} GBP''')

    if 'how many gbp does michael have' in message.content.lower() != -1:
        await message.channel.send(f'''Michael currently has {Michael} GBP''')

    if 'how many gbp does zameer have' in message.content.lower() != -1:
        await message.channel.send(f'''Zameer currently has {Zameer} GBP''')

    if 'how many gbp does logan have' in message.content.lower() != -1:
        await message.channel.send(f'''Logan currently has {Logan} GBP''')

    if 'how many gbp does haxy have' in message.content.lower() != -1:
        await message.channel.send(f'''HAXY currently has {HAXY} GBP''')


        #the GBP log
    if 'gbp log' in message.content.lower() != -1:
        await message.channel.send('-------'
            '\nNico - ' + str(Nico) + '\nEli - ' + str(Eli) + '\nAshton - ' + str(Ashton) + '\nLily - ' + str(
                Lily) + '\nEnrique - ' + str(Enrique) +
            '\nKylie - ' + str(Kylie) + '\nAbbie - ' + str(Abbie) + '\nLuke - ' + str(Luke) + '\nBrandon - ' + str(
                Brandon) +
            '\nCharlie - ' + str(Charlie) + '\nDan - ' + str(Dan) +
            '\nJJ - ' + str(JJ) + '\nMichael - ' + str(Michael) + '\nZameer - ' + str(Zameer) + '\nLogan - ' + str(Logan) + "\nHAXY - " + str(HAXY) + "\n-------")



#Allows for Transfer of GBP between different people
@client.listen()
async def on_message(message):
    global Nico
    global nico_author
    global Eli
    global eli_author
    global Ashton
    global ashton_author
    global Lily
    global lily_author
    global Enrique
    global enrique_author
    global Kylie
    global kylie_author
    global Abbie
    global abbie_author
    global Luke
    global luke_author
    global Brandon
    global Charlie
    global Dan
    global dan_author
    global JJ
    global jj_author
    global Michael
    global Zameer
    global zameer_author
    global Logan
    global logan_author
    global HAXY
    global haxy_author
    if str(message.author) == str(eli_author):
        if "give" in message.content.lower() and 'gbp' in message.content.lower() != -1:

            #index 0 should be 'give'
            #index 1 shoud be the number of gbp
            #index 2 should be 'gbp'
            #index 3 should be 'to'
            #index 4 should be the name of the person
            split_msg = str(message.content).split()
            print(type(Nico))
            print(split_msg[0])
            print(split_msg[1])
            print(split_msg[2])
            print(split_msg[3])
            print(split_msg[4])
            temp_gbp = int(split_msg[1])
            temp_gbp = abs(temp_gbp)
            print(temp_gbp)

            #Allows Eli to Transfer GBP to anyone else
            if int(split_msg[1]) <= int(Eli) and temp_gbp >= 0:
                if split_msg[4].lower() == "nico":
                    Nico += temp_gbp
                    Eli -= temp_gbp
                    await message.channel.send(f'Eli now has {Eli} GBP')
                    await message.channel.send(f'Nico now has {Nico} GBP')

                if split_msg[4].lower() == "ashton":
                    Ashton += abs(temp_gbp)
                    Eli -= abs(temp_gbp)
                    await message.channel.send(f'Eli now has {Eli} GBP')
                    await message.channel.send(f'Ashton now has {Ashton} GBP')

                if split_msg[4].lower() == "lily":
                    Lily += temp_gbp
                    Eli -= temp_gbp
                    await message.channel.send(f'Eli now has {Eli} GBP')
                    await message.channel.send(f'Lily now has {Lily} GBP')

                if split_msg[4].lower() == "enrique":
                    Enrique += temp_gbp
                    Eli -= temp_gbp
                    await message.channel.send(f'Eli now has {Eli} GBP')
                    await message.channel.send(f'Enrique now has {Enrique} GBP')

                if split_msg[4].lower() == "kylie":
                    Kylie += temp_gbp
                    Eli -= temp_gbp
                    await message.channel.send(f'Eli now has {Eli} GBP')
                    await message.channel.send(f'Kylie now has {Kylie} GBP')

                if split_msg[4].lower() == "abbie":
                    Abbie += temp_gbp
                    Eli -= temp_gbp
                    await message.channel.send(f'Eli now has {Eli} GBP')
                    await message.channel.send(f'Abbie now has {Abbie} GBP')

                if split_msg[4].lower() == "luke":
                    Luke += temp_gbp
                    Eli -= temp_gbp
                    await message.channel.send(f'Eli now has {Eli} GBP')
                    await message.channel.send(f'Luke now has {Luke} GBP')

                if split_msg[4].lower() == "brandon":
                    Brandon += temp_gbp
                    Eli -= temp_gbp
                    await message.channel.send(f'Eli now has {Eli} GBP')
                    await message.channel.send(f'Brandon now has {Brandon} GBP')

                if split_msg[4].lower() == "charlie":
                    Charlie += temp_gbp
                    Eli -= temp_gbp
                    await message.channel.send(f'Eli now has {Eli} GBP')
                    await message.channel.send(f'Charlie now has {Charlie} GBP')

                if split_msg[4].lower() == "dan":
                    Dan += temp_gbp
                    Eli -= temp_gbp
                    await message.channel.send(f'Eli now has {Eli} GBP')
                    await message.channel.send(f'Dan now has {Dan} GBP')

                if split_msg[4].lower() == "jj":
                    JJ += temp_gbp
                    Eli -= temp_gbp
                    await message.channel.send(f'Eli now has {Eli} GBP')
                    await message.channel.send(f'JJ now has {JJ} GBP')

                if split_msg[4].lower() == "michael":
                    Michael += temp_gbp
                    Eli -= temp_gbp
                    await message.channel.send(f'Eli now has {Eli} GBP')
                    await message.channel.send(f'Michael now has {Michael} GBP')

                if split_msg[4].lower() == "zameer":
                    Zameer += temp_gbp
                    Eli -= temp_gbp
                    await message.channel.send(f'Eli now has {Eli} GBP')
                    await message.channel.send(f'Zameer now has {Zameer} GBP')

                if split_msg[4].lower() == "logan":
                    Logan += temp_gbp
                    Eli -= temp_gbp
                    await message.channel.send(f'Eli now has {Eli} GBP')
                    await message.channel.send(f'Logan now has {Logan} GBP')

                if split_msg[4].lower() == "haxy":
                    HAXY += temp_gbp
                    Eli -= temp_gbp
                    await message.channel.send(f'Eli now has {Eli} GBP')
                    await message.channel.send(f'HAXY now has {HAXY} GBP')

            else:
                await message.channel.send(f"Looks like you don't have enough GBP")



     #Allows Ashton to transfer GBP to anybody else
